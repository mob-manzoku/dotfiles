remixd -s $PWD --remix-ide https://remix.ethereum.org
godoc $(ghq list --full-path | peco) | $PAGER
export DISPLAY=remote:0.1
goimports-update-ignore;dragon-imports
git branch | grep -v master | xargs git branch -d
sudo du -sh /home/* | sort -hr | column -t
find $HOME/src -maxdepth 2 -mindepth 2 -and -type d -and ! -name rmanzoku -and ! -name studyplus
fd -tf | xargs -I{} gixy --disable-includes {}  | less
docker images -f "dangling=true" -q | xargs docker rmi
mkr-hosts-colums poweroff | cut -d" " -f2 | xargs mkr retire
brew unlink solidity;brew install https://raw.githubusercontent.com/ethereum/homebrew-ethereum/f26f126820e5f47c3ed7ec6d5e6e046707443d87/solidity.rb # 0.4.25
